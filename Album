public class Album {
    private String name;
    private String artist;
    private int score;

    public Album(String name, String artist, int score) {
        this.name = name;
        this.artist = artist;
        this.score = score;
    }

    public String getName() {
        return name;
    }

    public String getArtist() {
        return artist;
    }

    public int getScore() {
        return score;
    }

    @Override
    public String toString() {
        return "Album: " + name + ", Artist: " + artist + ", Score: " + score;
    }
}
